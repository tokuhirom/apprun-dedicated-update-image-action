{
  "name": "apprun-dedicated-update-image-action",
  "version": "1.0.0",
  "description": "GitHub Action to update AppRun Dedicated application images",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "package": "ncc build src/main.ts -o dist --source-map --license licenses.txt",
    "all": "npm run build && npm run package",
    "local": "ts-node -r dotenv/config src/main.ts"
  },
  "keywords": [
    "actions",
    "apprun",
    "sakura"
  ],
  "author": "tokuhirom",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/http-client": "^2.2.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.10.0",
    "@vercel/ncc": "^0.38.1",
    "dotenv": "^17.2.3",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.0"
  }
}
